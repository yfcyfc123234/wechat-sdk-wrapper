<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/NavPage"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:padding="5dp">

    <EditText
        android:id="@+id/scene_et"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:hint="Input Scene"
        android:text="1000" />

    <EditText
        android:id="@+id/templateid_et"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:hint="Input Template ID"
        android:text="Jo-ywGDy0K9zGY87D2Cs2D51ExMoA1xSor7UxfIiLG8" />

    <EditText
        android:id="@+id/reserved_et"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:hint="Input Reserved"
        android:text="abcAbc" />

    <Button
        android:id="@+id/check_subscribe_message_btn"
        style="@style/MMLineActionButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/check_subscribe_message" />

    <Button
        android:id="@+id/subscribe_message_btn"
        style="@style/MMLineActionButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/subscribe_message" />

</LinearLayout>
